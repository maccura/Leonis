#CupHistoryWidget ReactionDiskWidget {
    qproperty-centerX:400;							/* 圆环中心点X坐标 */
    qproperty-centerY:400;							/* 圆环中心点Y坐标 */
    qproperty-sectorCount:17;						/* 扇区个数 */
    qproperty-sectorSpacingAngle:1;					/* 扇区的间隔角度 */
    qproperty-sectorClickedWidth:75;				/* 扇区按钮的宽度 */
	qproperty-sectorWidth:98;						/* 扇区的宽度 */
	qproperty-sectorHeight:123;						/* 扇区的高度 */
    qproperty-startAngle:196;						/* 1号扇区的起始角度（三点钟方向为0度,逆时针为正） */
    qproperty-outR:350;								/* 最大圆半径 */
    qproperty-cupToSectorMargins:0.75;				/* 反应杯距离扇区的边距的度数 */
	qproperty-cupNumInItem:13;						/* 单个扇区中的反应杯个数 */
	qproperty-cupToOutRoundMargins:5;				/* 杯子到外圈的距离 */
    qproperty-cupAngle:0.65;						/* 一个杯子所占大圆的角度 */
    qproperty-cupNumMargins:40;						/* 杯号显示区域到外圆的距离 */
    qproperty-cupNumRectWidth:30;					/* 杯号显示区域矩形的宽度 */
	qproperty-cupNumFontFamily: "Source Han Sans CN Regular";	/* 杯号的字体 */
	qproperty-cupNumFontSize: 12;					/* 杯号的字体大小 */
	qproperty-cupNumColor: rgb(107,108,109);		/* 杯号未选中的字体颜色 */
	qproperty-cupNumSelectColor: rgb(0,0,0);		/* 杯号选中的字体颜色 */
}

