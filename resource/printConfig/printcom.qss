/* 模块列表 */
widget--printcom_settings_window QComboBox#module_list {
	background-color: #ffffff;
	border-radius: 4px;
	border: 1px solid #a4a4a4;	
	padding-left: 10px;
	padding-right: 10px;
	font-size: 24px;
	color: #565656;
}

widget--printcom_settings_window QComboBox#module_list:on {
	border: 1px solid #1b7df5;	
}

widget--printcom_settings_window QComboBox#module_list:hover {
	/*border: 1px solid #025bc7;*/
	border: 1px solid #1b7df5;	
}

/* 下拉后,整个下拉窗体样式 */
widget--printcom_settings_window QComboBox#module_list QAbstractItemView {
    outline: 0px solid #459fff;   /* 选定项的虚框 */
    border: 1px solid #a4a4a4;   /* 整个下拉窗体的边框 */
    background-color: #ffffff;   /* 整个下拉窗体的背景色 */
	selection-background-color: #e5f5ff;   /* 整个下拉窗体被选中项的背景色 */
    selection-color: #565656;  
	font-size: 18px;
	color: #565656;
}

/* 下拉后,整个下拉窗体每项的样式 */
widget--printcom_settings_window QComboBox#module_list QAbstractItemView::item {
    height: 56px;   /* 项的高度（设置pComboBox->setView(new QListView());后,该项才起作用） */
}

/* 下拉后,整个下拉窗体越过每项的样式 */
widget--printcom_settings_window QComboBox#module_list QAbstractItemView::item:hover {
	color: #565656;
	border: 1px solid #025bc7;
}

widget--printcom_settings_window QComboBox#module_list:drop-down {
	border-left-width: 0px;
}

widget--printcom_settings_window QComboBox#module_list:down-arrow {
	width: 12px;
	height: 10px;
	margin-right: 10px;
	image: url(":/drop_down");
}

widget--printcom_settings_window QComboBox#module_list:down-arrow:on {
	image: url(":/drop_down");
}

