}

/*设置垂直滚动的全局属性值*/
widget--printcom_settings_window QListWidget#template_list QScrollBar:vertical,
widget--printcom_settings_window QComboBox#module_list QScrollBar:vertical,
widget--printcom_integration_window QComboBox#printer_box QScrollBar:vertical,
widget--default_templates_overview_window QTableWidget#template_table QScrollBar:vertical,
widget--printer_settings_window QComboBox#printer_box QScrollBar:vertical
{
    width:13px;
    /*height: 629px;*/
	border: none;
    background-color:#e4e7ec;
    margin:0px,0px,0px,0px;
    padding-top:0px;
    padding-bottom:0px;
}
/* 滑块区域样式 */
widget--printcom_settings_window QListWidget#template_list QScrollBar::handle:vertical,
widget--printcom_settings_window QComboBox#module_list QScrollBar::handle:vertical,
widget--printcom_integration_window QComboBox#printer_box QScrollBar::handle:vertical,
widget--default_templates_overview_window QTableWidget#template_table QScrollBar::handle:vertical,
widget--printer_settings_window QComboBox#printer_box QScrollBar::handle:vertical {
	width: 9px;
	/*height: 150px;*/
	background-color: #ffffff;
	border: 0px solid  #a4a4a4;
	border-radius: 5px;
  	margin:0 1px 0 1px;
}
/* 向上区域样式 */
widget--printcom_settings_window QListWidget#template_list QScrollBar::sub-line:vertical,
widget--printcom_settings_window QComboBox#module_list QScrollBar::sub-line:vertical,
widget--printcom_settings_window QComboBox#module_list QScrollBar::sub-line:vertical,
widget--default_templates_overview_window QTableWidget#template_table QScrollBar::sub-line:vertical,
widget--printer_settings_window QComboBox#printer_box QScrollBar::sub-line:vertical {
	width: 0px;
	height:0px;
	background-color: #e4e7ec;
  	subcontrol-position: top;
 	subcontrol-origin: margin;
}

/* 向下区域样式 */
widget--printcom_settings_window QListWidget#template_list QScrollBar::add-line:vertical,
widget--printcom_settings_window QComboBox#module_list QScrollBar::add-line:vertical,
widget--printcom_settings_window QComboBox#module_list QScrollBar::add-line:vertical,
widget--default_templates_overview_window QTableWidget#template_table QScrollBar::add-line:vertical,
widget--printer_settings_window QComboBox#printer_box QScrollBar::add-line:vertical {
	width:0px;
	height:0px;
	background-color: #e4e7ec;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

/* 滚动时上下区域样式 */
widget--printcom_settings_window QListWidget#template_list QScrollBar::add-page:vertical,
widget--printcom_settings_window QListWidget#template_list QScrollBar::sub-page:vertical,
